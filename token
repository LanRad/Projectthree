lm_function <- function(dataf,col_dep, col_indep){
  dataf %>%
  if(is.numeric({{col_dep}}))  {
    return_summary <- dataf %>%
      select(a = quo_name(col_dep), starts_with(col_indep)) %>%
      lm(a ~ ., data = .) %>%
      summary()
    return(return_summary)
  } else {
    print("Response not numeric, not appropriate for LM")
  }

   }
